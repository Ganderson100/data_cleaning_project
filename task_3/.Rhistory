select(species, count) %>%
group_by(species) %>%
summarise(birds_seen_in_groups_of_1 = n()) %>%
filter(birds_seen_in_groups_of_1 == 1)
lonely_birds
# count of the number of different bird species seen in groups of one
count(lonely_birds)
